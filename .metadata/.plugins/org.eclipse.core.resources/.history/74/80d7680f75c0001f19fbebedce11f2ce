class System {
	val allAccessDevices : List[AccessDevice] = List()
	val currentAccessDevices : Set[AccessDevice] = Set()
	val bannedAccessDevices : Set[AccessDevice] = Set()
	
	val accounts : Set[Account] = Set()
	
	val statusTypes : Set[Status] = Set()
	val concessions : Set[Concession] = Set()
	
	def banAccessDevice(ad:AccessDevice) {
		if (currentAccessDevices.contains(ad)) {
			currentAccessDevices -= ad
			bannedAccessDevices  += ad
		}
	}

}